{
  "schemaVersion": 1,
  "id": "emotecraft3",
  "version": "${version}",

  "name": "Emotecraft 3.0",
  "description": "Emotecraft for a simpler, better age",
  "authors": [
    "KosmX"
  ],
  "contact": {
    "homepage": "https://kosmx.dev/",
    "sources": "https://github.com/KosmX/emotecraft3.0"
  },

  "license": "CC0-1.0",

  "environment": "*",
  "entrypoints": {
    "main": [
      {
        "adapter": "kotlin",
        "value": "dev.kosmx.emotecraft3.ModLoader"
      }
    ],
    "client": [
      {
        "adapter": "kotlin",
        "value": "dev.kosmx.emotecraft3.client.ClientModLoader"
      }
    ]
  },
  "mixins": [
    "emotecraft3.mixins.json"
  ],

  "depends": {
    "fabricloader": ">=0.14.17",
    "fabric-api": "*",
    "fabric-language-kotlin": ">=1.9.3+kotlin.1.8.20",
    "minecraft": ">=1.19.4",
    "java": ">=17"
  },
  "suggests": {
    "another-mod": "*"
  }
}
